{
  "expo": {
    "name": "Desk Yoga",
    "slug": "desk-yoga",
    "scheme": "desk-yoga",
    "version": "1.0.0",
    "splash": {
      "backgroundColor": "#000000",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.cybergoose.deskyoga",
      "buildNumber": "0",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "org.cybergoose.deskyoga",
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF"
      }
    },
    "newArchEnabled": true,
    "plugins": [
      "expo-font",
      [
        "react-native-iap",
        {
          "ios": {
            "enableStoreKit2": true
          },
          "android": {
            "store": "play"
          }
        }
      ],
      "expo-router"
    ],
    "icon": "./assets/images/icon.png",
    "owner": "ernkerr",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2e786503-fade-4c13-8491-7d0655f209a9"
      }
    }
  }
}
